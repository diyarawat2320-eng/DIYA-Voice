{
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.py"
        },
        {
            "src": "/favicon.ico",
            "dest": "/api/index.py"
        },
        {
            "src": "/(.*\\.css)",
            "dest": "/$1"
        },
        {
            "src": "/(.*\\.js)",
            "dest": "/$1"
        },
        {
            "src": "/(.*\\.html)",
            "dest": "/$1"
        },
        {
            "src": "/(.*\\.png|.*\\.jpg|.*\\.svg|.*\\.ico)",
            "dest": "/$1"
        },
        {
            "src": "/",
            "dest": "/index.html"
        }
    ]
}